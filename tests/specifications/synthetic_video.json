{
  "directed": true,
  "graph": {
    "username": "unicorn",
    "name": "synthetic_video",
    "organization": "Wonderland",
    "projectdescription" : "Runs some basic operation to turn a Synthetic Video into an extendable journal",
    "technicalsummary" : "Automated: Synthetic video media",
    "semanticrestaging" : "no",
    "semanticrepurposing" : "no",
    "semanticrefabrication" : "no",
    "recompress": false
  },
  "nodes":[
	{
      "op_type": "BaseSelection",
      "image_directory": "{image_dir}",
      "picklist": "videoset",
      "id": "Base",
      "arguments": {
        "cgi":"yes"
      },
      "semanticGroups":["Drone Video Tests"]
    },
	{
	  "op_type": "PreProcessedMediaOperation",
	  "id": "Synthesize",
	  "arguments":{
		"urls":  "https://www.nonesence.com",
        "Recording Software": {
          "type" : "value",
          "value": "Name of software to recapture screen"
        },
        "KML File": {
          "type" : "value",
          "value": "file.kml"
        },
        "Rendering Software": {
          "type" : "value",
          "value" : "rendering software"
        },
        "Time Settings": {
          "type" : "value",
          "value" : "Default time"
        }
      },
	  "description":"Generating a synthetic video",
      "argument file": "PAR_Video_Sample.txt",
	  "directory": "{AllDir}",
	  "op": "SynthesizeVideo",
      "plugin": "None",
	  "category": "CGI",
	  "software": "SyntheticVidSpec",
	  "software version": "1.0",
      "copy": true,
	  "argument names": ["Rendering Software","Recording Software","Video Location","KML File","urls","Time Settings","Time of Sun"]
	},
    {
	  "op_type": "PluginOperation",
      "plugin": "OutputCopy",
      "id": "SynthCopy",
      "arguments": {
      }
	},
	{
	  "op_type": "PluginOperation",
      "plugin": "FrameDuplicateDrop",
      "id": "FrameDropped",
      "arguments": {
	    "Threshold": {
          "type" : "value",
          "value" : 3
        }
      }
	},
    {
      "op_type": "ImageSelection",
      "id":"SelectDJI",
      "files":["{PRNU}/Drone_InspireFC350_PAR-FC350_20180507-190057_15.mov"],
      "prnu": true
    },
    {
      "op_type": "PluginOperation",
      "plugin": "CompressAsVideo",
      "id": "PRNULess",
      "arguments": {
        "donor": {
          "type" : "donor",
          "source": "SelectDJI"
        }
      }
	},
    {
      "op_type": "PluginOperation",
      "plugin": "PRNUFromDonorVid",
      "id":"PRNUAdded",
      "arguments": {
        "donor": {
          "type" : "donor",
          "source" : "SelectDJI"
        },
        "Capture Camera ID" : {
          "type" : "value",
          "value" : "PAR-FC350"
        },
        "Single":{
          "type": "value",
          "value": "yes"
        }
      }
    },
	{
      "op_type": "PluginOperation",
      "plugin": "CompressAsVideo",
      "id": "PRNUOutput",
      "arguments": {
        "donor": {
          "type" : "donor",
          "source": "SelectDJI"
        }
      }
	}
  ],
  "links":[
	{
	  "source":"Base",
	  "target":"Synthesize"
	},
    {
      "source": "Synthesize",
      "target": "SynthCopy"
    },
	{
	  "source":"Synthesize",
	  "target":"FrameDropped"
	},
    {
      "source":"Base",
      "target":"SelectDJI",
      "connect": false
    },
    {
      "source":"SelectDJI",
      "target":"PRNUAdded",
      "connect": false
    },
    {
      "source":"SelectDJI",
      "target":"PRNUOutput",
      "connect": false
    },
    {
      "source":"FrameDropped",
      "target":"PRNUAdded"
    },
    {
      "source": "PRNUAdded",
